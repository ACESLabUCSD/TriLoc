set search_path [list . ../TinyGarbled/gen_netlist/lib/dff_full/]
set target_library ../TinyGarbled/gen_netlist/lib/dff_full/dff_full.db
set link_library ../TinyGarbled/gen_netlist/lib/dff_full/dff_full.db
set symbol_library [concat ../TinyGarbled/gen_netlist/lib/generic.sdb]
set hdlin_while_loop_iterations 2049

analyze -format verilog {../TinyGarbled/gen_netlist/syn_lib/MUX.v ../TinyGarbled/gen_netlist/syn_lib/FA.v ../TinyGarbled/gen_netlist/syn_lib/ADD.v ../TinyGarbled/gen_netlist/syn_lib/ADD_.v ../TinyGarbled/gen_netlist/syn_lib/SUB.v  ../TinyGarbled/gen_netlist/syn_lib/SUB_.v   ../TinyGarbled/gen_netlist/syn_lib/TwosComplement.v ../TinyGarbled/gen_netlist/syn_lib/MULT.v ../TinyGarbled/gen_netlist/syn_lib/MULT_.v  ../TinyGarbled/gen_netlist/syn_lib/DIV.v ../TinyGarbled/gen_netlist/syn_lib/DIV_.v ../TinyGarbled/gen_netlist/syn_lib/COMP.v ../TinyGarbled/gen_netlist/syn_lib/COUNT.v ../TinyGarbled/gen_netlist/syn_lib/square_root_comb.v }

analyze -format verilog {intersections.v}

elaborate intersections -architecture verilog -library DEFAULT -update 
set_max_area -ignore_tns 0 
#ungroup -flatten -all
set_flatten false -design *
set_structure -design * false
set_resource_allocation area_only
report_compile_options
compile -ungroup_all  -map_effort low -area_effort low -no_design_rule
write -hierarchy -format verilog -output syn/intersections_netlist.v

exit
